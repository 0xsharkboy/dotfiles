set number
set colorcolumn=80
set tabstop=4
set shiftwidth=4
set expandtab
set mouse=a
inoremap " ""<left>
inoremap ' ''<left>
inoremap ( ()<left>
inoremap [ []<left>
inoremap { {}<left>
inoremap {<CR> {<CR>}<ESC>O
inoremap {;<CR> {<CR>};<ESC>O
syntax on
set cursorline
set clipboard=unnamedplus
start